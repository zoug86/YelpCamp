<% layout('layouts/boilerplate') %>
    <div class="row">
        <div class="col-md-6 offset-3">
            <div class="alert alert-danger" role="alert">
                <h4 class="alert-heading">
                    <%= err.message %>
                </h4>
                <% if(process.env.NODE_ENV !=="poduction" ) { %>
                    <p>
                        <%= err.stack %>
                    </p>
                    <% } %>
            </div>
        </div>